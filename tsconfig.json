{
  "compilerOptions": {
    "resolveJsonModule": true,
    /* Basic Options */
    "target": "es5",
    "module": "commonjs",
    // "lib": ["DOM"],
    "jsx": "react-jsx",
    /* Strict Type-Checking Options */
    "strict": true,
    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,
    /* Advanced Options */
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
